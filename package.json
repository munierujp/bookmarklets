    "@types/glob": "8.0.0",
    "@types/node": "16.11.66",
    "eslint": "8.25.0",
    "eslint-plugin-jest": "27.1.3",
    "glob": "8.0.3",
    "jest": "29.2.0",
    "jest-environment-jsdom": "29.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-watch": "1.0.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
